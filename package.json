{
  "name": "docgen",
  "description": "Documentation generator.",
  "version": "1.0.0",
  "author": "Paul Knopf <pauldotknopf@gmail.com>",
  "private": true,
  "scripts": {
    "bootstrap": "./node_modules/.bin/lerna bootstrap",
    "build": "./node_modules/.bin/lerna run build",
    "watch": "./node_modules/.bin/lerna run build:watch --parallel",
    "docgen": "./packages/docgen-cli/index.js",
    "test": "./node_modules/.bin/mocha packages/**/__tests__/*",
    "pub": "./node_modules/.bin/lerna publish"
  },
  "dependencies": {
    "gatsby": "1.9.250",
    "react-helmet": "5.2.0"
  },
  "devDependencies": {
    "lerna": "2.10.1",
    "mocha": "5.1.0",
    "mock-fs": "4.4.2",
    "assert-throws-async": "2.0.1"
  },
  "workspaces": [
    "packages/*"
  ]
}
